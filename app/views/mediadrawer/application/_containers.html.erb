<div class="row">
  <div class="col-xs-11">
    <ul class="nav nav-tabs" role="tablist">
      <li class="active"><a href="#files" role="tab" data-toggle="tab">Arquivos</a></li>
      <li><a href="#upload" role="tab" data-toggle="tab">Upload</a></li>
    </ul>
  </div>
</div>
<div class="row">
  <div class='col-xs-12'>
    <div class="row">
      <div class="col-xs-2">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
          <div class='folder-menu col-xs-2' data-folder-menu>
            <ul class="sidebar-nav"></ul>
          </div>
        </div>
        <!-- /#sidebar-wrapper -->
      </div>
      <div class="col-xs-10">
        
      
    <div class="tab-content">

      <div class="tab-pane active" id="files">
        <div class='col-xs-12 container' data-image-container>
          <ul class="row thumbnails"></ul>
        </div>
      </div>

      <div class="tab-pane" id="upload">
        <form action="<%= media_index_path %>" method="POST" enctype="multipart/form-data">
          <div class="row">
            <div class="col-xs-8">
              <div class="row">
                <div class="col-xs-12">
                  <input type="file" name="file" class="file">
                </div>
              </div>
              <hr />
              <div class="row">
                <div class="col-xs-12">
                  URL: <input type="url" name="link" class="form-control">
                </div>
              </div>
            </div>
            <div class="col-xs-4">
              <button class="btn btn-primary" data-async-submit data-success-fn="uploadSuccess" data-progress-fn='uploadProgress'>Upload</button>
            </div>
          </div>
          <div class="row">
            <div class="col-xs-12">
              <progress style="display: none"></progress>
            </div>
          </div>
        </form>
      </div>

    </div>

</div>
    </div>

  </div>
</div>